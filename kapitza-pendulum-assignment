        "# input parameters\n",
        "epsilon = 0.2 # dimensionless angular frequency\n",
        "alpha = 0.3 # dimensionless drive strength\n",
        "zeta = 0.15 # dimensionless damping\n",
        "\n",
        "# number of decay times we want to integrate over\n",
        "n_decay = 1000\n",
        "\n",
        "# Call cos_theta_averaging\n",
        "cos_theta_averaging, solution = average_timescale_response(epsilon, alpha, zeta, n_decay=n_decay, delta_theta=0.001)\n",
        "\n",
        "# print averaged long time scale response\n",
        "print('<cos(theta)>=', cos_theta_averaging)\n",
        "\n",
        "# plot pendulum dynamics\n",
        "tau_min = 0\n",
        "tau_max = n_decay/(2*zeta)\n",
        "tau_plot = np.linspace(tau_min, tau_max, 1000)\n",
        "theta_plot = solution.sol(tau_plot)[0]\n",
        "\n",
        "# basic_plot(tau_plot, theta_plot, x_label=r'$\\tau$', y_label='$\\theta$', figsize=(8,2))\n",
        "plt.figure(figsize=(8,2))\n",
        "plt.plot(tau_plot, theta_plot)\n",
        "plt.xlabel(r'$\\tau$')\n",
        "plt.ylabel(r'$\\theta$')\n",
        "plt.show()\n",
        "\n",
        "# plot theta\n",
        "plt.figure(figsize=(8,2))\n",
        "plt.plot(tau_plot, np.cos(theta_plot))\n",
        "plt.xlabel(r'$\\tau$')\n",
        "plt.ylabel(r'$\\cos(\\theta)$')\n",
        "plt.ylim(-1.1, 1.1)\n",
        "plt.show()\n",
        "\n",
        "plt.figure(figsize=(8,2))\n",
        "plt.plot(tau_plot, np.sin(theta_plot))\n",
        "plt.xlabel(r'$\\tau$')\n",
        "plt.ylabel(r'$\\sin(\\theta)$')\n",
        "plt.ylim(-1.1, 1.1)\n",
        "plt.show()"
